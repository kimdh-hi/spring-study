type User {
    id: ID
    name: String!
    username: String!
}

input UserSaveRequest {
    name: String!
    username: String!
}

type Query {
    findUser(id: String!): User
    findUsers: [User]
}

type Mutation {
    saveUserV1(name: String!): User
    saveUserV2(userSaveRequest: UserSaveRequest): User
}